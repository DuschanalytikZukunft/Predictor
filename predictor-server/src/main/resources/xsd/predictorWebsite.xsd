           targetNamespace="urn:beans.predictor.com">

    <xs:complexType name="predictorWebsite">
        <xs:sequence>
            <xs:element name="Date" type="xs:string"/>
            <xs:element name="Timestamp" type="xs:string"/>
            <xs:element name="PreviousDate" type="xs:string"/>
            <xs:element name="PreviousUrl" type="xs:string"/>
            <xs:element name="ValueEUR" type="xs:double"/>
            <xs:element name="PreviousEUR" type="xs:double"/>
            <xs:element name="CharCodeEUR" type="xs:string"/>
            <xs:element name="ValueUSD" type="xs:double"/>
            <xs:element name="PreviousUSD" type="xs:double"/>
            <xs:element name="CharCodeUSD" type="xs:string"/>
